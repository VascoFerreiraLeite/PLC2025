[
  {
    "id": "SELECT",
    "expreg": "\\bselect\\b"
  },
  {
    "id": "WHERE",
    "expreg": "\\bwhere\\b"
  },
  {
    "id": "LIMIT",
    "expreg": "\\blimit\\b"
  },
  {
    "id": "VAR",
    "expreg": "\\?[A-Za-z_]\\w*"
  },
  {
    "id": "NUM",
    "expreg": "\\d+"
  },
  {
    "id": "NOME",
    "expreg": "[A-Za-z_][\\w\\-]*:[A-Za-z_][\\w\\-]*(?:/[A-Za-z0-9_\\-./]*)?"
  },
  {
    "id": "STRING",
    "expreg": "\"(?:[^\"\\\\]|\\\\.)*\"(?:@[a-zA-Z\\-]+)?"
  },
  {
    "id": "BRACE_A",
    "expreg": "\\{"
  },
  {
    "id": "RBRACE_F",
    "expreg": "\\}"
  },
  {
    "id": "PONTO",
    "expreg": "\\."
  },
  {
    "id": "VIRGULA",
    "expreg": ","
  },
  {
    "id": "PONTO_VIRGULA",
    "expreg": ";"
  },
  {
    "id": "DOIS_PONTOS",
    "expreg": ":"
  },
  {
    "id": "IDEN",
    "expreg": "[A-Za-z_][\\w\\-]*"
  },
  {
    "id": "SKIP",
    "expreg": "[ \\t]+"
  },
  {
    "id": "NEWLINE",
    "expreg": "\\n"
  },
  {
    "id": "COMMENT",
    "expreg": "#.*"
  },
  {
    "id": "ERRO",
    "expreg": "."
  }
]
