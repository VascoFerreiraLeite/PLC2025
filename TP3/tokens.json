[
    {"id": "KW_SELECT", "expreg": "(?i)\\bSELECT\\b"},
    {"id": "KW_CONSTRUCT", "expreg": "(?i)\\bCONSTRUCT\\b"},
    {"id": "KW_DESCRIBE", "expreg": "(?i)\\bDESCRIBE\\b"},
    {"id": "KW_ASK", "expreg": "(?i)\\bASK\\b"},
    {"id": "KW_WHERE", "expreg": "(?i)\\bWHERE\\b"},
    {"id": "KW_PREFIX", "expreg": "(?i)\\bPREFIX\\b"},
    {"id": "KW_BASE", "expreg": "(?i)\\bBASE\\b"},
    {"id": "VAR", "expreg": "\\?[A-Za-z_][A-Za-z0-9_\\-]*"},
    {"id": "IRI", "expreg": "<[^>\\n]*>"},
    {"id": "LITERAL", "expreg": "\"(?:\\\\\\\\.|[^\"\\\\\\\\])*\"(?:@[a-zA-Z\\-]+|\\^\\^<[^>\\n]*>)?"},
    {"id": "NUMBER", "expreg": "[+-]?(?:[0-9]+(?:\\.[0-9]+)?|\\.[0-9]+)(?:[eE][+-]?[0-9]+)?"},
    {"id": "PNAME_NS", "expreg": "[A-Za-z][A-Za-z0-9_\\-]*:"},
    {"id": "PNAME_LN", "expreg": "[A-Za-z][A-Za-z0-9_\\-]*:[A-Za-z0-9_\\-]+"},
    {"id": "LBRACE", "expreg": "\\{"},
    {"id": "RBRACE", "expreg": "\\}"},
    {"id": "LPAREN", "expreg": "\\("},
    {"id": "RPAREN", "expreg": "\\)"},
    {"id": "DOT", "expreg": "\\."},
    {"id": "SEMICOLON", "expreg": ";"},
    {"id": "COMMA", "expreg": ","},
    {"id": "EQ", "expreg": "="},
    {"id": "NEQ", "expreg": "!="},
    {"id": "LT", "expreg": "<"},
    {"id": "GT", "expreg": ">"},
    {"id": "LE", "expreg": "<="},
    {"id": "GE", "expreg": ">="},
    {"id": "SKIP", "expreg": "[ \\t\\r\\n]+", "skip": true},
    {"id": "COMMENT", "expreg": "#[^\\n]*", "skip": true}
  ]
